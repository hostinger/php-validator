{
    "name": "hostinger/php-validator",
    "description": "Array value validation library for PHP",
    "type": "library",
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:hostinger/travers.git"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "hostinger/travers": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Luur\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luur\\Validator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
